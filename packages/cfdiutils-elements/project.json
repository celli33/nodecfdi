{
  "root": "packages/cfdiutils-common",
  "sourceRoot": "packages/cfdiutils-common/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/cfdiutils-common",
        "tsConfig": "packages/cfdiutils-common/tsconfig.lib.json",
        "packageJson": "packages/cfdiutils-common/package.json",
        "main": "packages/cfdiutils-common/src/index.ts",
        "assets": ["packages/cfdiutils-common/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/cfdiutils-common/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/cfdiutils-common"],
      "options": {
        "jestConfig": "packages/cfdiutils-common/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
